# Archivo: home/.htaccess
RewriteEngine On

RewriteCond %{REQUEST_FILENAME} -f
RewriteCond %{REQUEST_FILENAME} \.php$
RewriteRule ^ - [L]

# Si el archivo existe (cualquier tipo), mostrarlo
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule ^ - [L]

# Si el directorio existe, mostrarlo
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

RewriteRule ^(.*)$ index.php [L,QSA]